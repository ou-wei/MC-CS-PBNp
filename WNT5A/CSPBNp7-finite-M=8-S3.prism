endmodule

module  BN1
x1:[0..1];
//d1:[0..1];
[csPBN]per1=0->(x1'=f11);
[csPBN]per1=1&x1=0->(x1'=1);
[csPBN]per1=1&x1=1->(x1'=0);
endmodule

