//WNT5A
mdp
formula   f11=(1-x6);
//formula    f12=x1;

formula    f21=((1-x2)*x4*x6)+(x2*(x4+x6-x4*x6))-((1-x2)*x4*x6)*(x2*(x4+x6-x4*x6));

formula   f31=(1-x7);
formula   f32=x3;

formula   f41=x4;


formula   f51=(1-x7)+x2-(1-x7)*x2;
//formula   f52=x5;

formula   f61=x3+x4-x3*x4;
//formula   f62=x6;

formula   f71=x7+(1-x2)-x7*(1-x2);
formula   f72=x7;
 
module  switch
